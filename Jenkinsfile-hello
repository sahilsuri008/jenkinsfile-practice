pipeline {
  agent any
  stages {
    stage ('checkout scm') {
      steps {
        git branch: 'main', credentialsId: '1cb6bd51-d866-4229-8549-1698375b867a', url: 'https://github.com/sahilsuri008/jenkinsfile-practice.git'
       }
     }
    stage ('HelloWorld') {
      steps {
        echo "My first Jenkins job"
        sh "uname -a"
        }
      }
   }
}
